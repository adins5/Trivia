<Window x:Class="Client.HighScore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="HighScore" Height="450" Width="800">
    <Grid>
        <Button Content="menu" Click="Menu_Click" Width="70" Height="40" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="20"/>
        <TextBlock x:Name="Title" HorizontalAlignment="Center" Text="Top Players" FontSize="30" TextAlignment="Center" Margin="40"/>
        <StackPanel x:Name="Players"  HorizontalAlignment="Center" VerticalAlignment="Center">

        </StackPanel>
    </Grid>
</Window>
